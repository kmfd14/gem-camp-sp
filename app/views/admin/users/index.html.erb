<h1>Admin Users</h1>
<table class="table table-striped table-hover">
  <thead>
  <td>parent email</td>
  <td>email</td>
  <td>total deposit</td>
  <td>member total deposits</td>
  <td>coins</td>
  <td>total used coins</td>
  <td>children members</td>
  <td>phone number</td>
  </thead>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.parent_id %></td>
      <td><%= user.email %></td>
      <td><%= user.total_deposit %></td>
      <td></td>
      <td><%= user.coins %></td>
      <td></td>
      <td><%= user.children_member %></td>
      <td><%= user.phone_number %></td>
    </tr>
  <% end %>
</table>